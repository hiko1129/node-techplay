{
  "rulesDirectory": ["tslint-plugin-prettier"],
  "extends": ["tslint:recommended", "tslint-config-standard", "tslint-config-prettier"],
  "rules": {
      "object-literal-sort-keys": false,
      "interface-name": [true, "never-prefix"],
      "prettier": [
      true,
      {
          "singleQuote": true,
          "semi": false,
          "trailingComma": "none"
      }
      ]
  }
}